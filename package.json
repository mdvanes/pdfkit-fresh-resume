{
  "name": "pdfkit-fresh-resume",
  "version": "1.1.0",
  "description": "Convert a resume (CV) in FRESH format to PDF with PDFKit. This includes an example resume JSON and an example theme.",
  "homepage": "https://github.com/mdvanes/pdfkit-fresh-resume",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdvanes/pdfkit-fresh-resume.git"
  },
  "bugs": {
    "url": "https://github.com/mdvanes/pdfkit-fresh-resume/issues"
  },
  "main": "index.js",
  "bin": {
    "pdfkit-fresh-resume": "index.js"
  },
  "scripts": {
    "transform": "node_modules/.bin/eslint app/{util,template}/*.js && node --harmony app/transformer/pdf.js example-resume.json example-output"
  },
  "author": "M.D. van Es",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^3.15.0",
    "eslint-loader": "^1.6.1"
  },
  "dependencies": {
    "datauri": "^1.0.5",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "pdfkit": "^0.8.0",
    "watch": "^1.0.2",
    "webpack": "^4.19.0",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-hot-middleware": "^2.24.0",
    "yargs": "^12.0.2"
  }
}
